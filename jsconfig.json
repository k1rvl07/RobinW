{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "@assets/*": [
                "assets/*"
            ],
            "@components/*": [
                "components/*"
            ],
            "@hooks/*": [
                "hooks/*"
            ],
            "@pages/*": [
                "pages/*"
            ],
            "@img/*": [
                "assets/img/*"
            ],
            "@svg/*": [
                "assets/svg/*"
            ],
            "@style_scss/*": [
                "style_scss/*"
            ],
            "@data/*": [
                "data/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}