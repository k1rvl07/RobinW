{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "@assets/*": [
                "assets/*"
            ],
            "@components/*": [
                "components/*"
            ],
            "@hooks/*": [
                "hooks/*"
            ],
            "@pages/*": [
                "pages/*"
            ],
            "@img/*": [
                "assets/img/*"
            ],
            "@svg/*": [
                "assets/svg/*"
            ],
            "@style/*": [
                "style/*"
            ],
            "@data/*": [
                "data/*"
            ],
            "@modules/*": [
                "modules/*"
            ],
            "@exports/*": [
                "modules/exports/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}